- var notes = ''
section(data-background="#282c34").dark
  section
    h2 Medien
  section
    h3 Bilder
    table
      thead
        tr
          th Format
          th Datei
      tbody
        tr
          td JPEG
          td .jpg \ .jpeg
        tr
          td GIF
          td .gif
        tr
          td ICO
          td .ico
        tr
          td PNG
          td .png
        tr
          td SVG
          td .svg
        tr
          td WEBP
          td .webp
  section
    h3 Video
    table
      thead
        tr
          th Format
          th Datei
      tbody
        tr
          td MPEG-4 or MP4
          td .mp4
        tr
          td WebM
          td .webm
        tr
          td Ogg
          td .ogg
  section
    h3 Audio
    table
      thead
        tr
          th Format
          th Datei
      tbody
        tr
          td MP4
          td .mp4
        tr
          td OGG
          td .ogg
        tr
          td MP3
          td .mp3
        tr
          td WAV
          td .wav

  section
    h2 Images revisited
    pre
      code.html.
        &lt;img src="url" alt="alternatetext"&gt;
    pre.fragment
      code.html.
        &lt;img src="url"
          srcset="image-480.png 480w,
                  image-920.png 920w,
                  image-1080.png 1080w,"
          alt="alternatetext"&gt;
    pre.fragment
      code.html.
        &lt;picture&gt;
          &lt;source media="(min-width: 650px)" srcset="image-650.jpg"&gt;
          &lt;source media="(min-width: 465px)" srcset="image-465.jpg"&gt;
          &lt;img src="image.jpg"&gt;
        &lt;/picture&gt;
  section
    h2 Audio
    pre
      code.html.
       &lt;audio controls&gt;
         &lt;source src="horse.ogg" type="audio/ogg"&gt;
         &lt;source src="horse.mp3" type="audio/mpeg"&gt;
        Your browser does not support the audio element.
       &lt;/audio&gt;
  section
    .split
      .left
        h4 index.html
        pre
          code.html.
            &lt;html&gt;
              &lt;head&gt;
              &lt;/head&gt;
              &lt;body&gt;
                &lt;header&gt;
                &lt;/header&gt;
                &lt;main&gt;
                  &lt;audio src="https://howtoweb3.de/media/doofenschmirtz.mp3", controls/&gt;
                  &lt;audio src="https://howtoweb3.de/media/imperialMarch.mp3", controls/&gt;
                  ...
                &lt;/main&gt;
              &lt;/body&gt;
            &lt;/html&gt;
            

      .right
        +preview('file://index.html',[{title:'Doofenschmirtz Evil Inc.', active:true},{title:'Preise - Doofenschmirtz Evil Inc.', active:false},{title:'Bilder - Doofenschmirtz Evil Inc.', active:false}])
          style.
            * {
              box-sizing: border-box;
            }
            body{
              font-family: "Ebrima";
              font-size: 16px;
              background: #eee;
              margin: 0;
              padding: 0;
            }
          
            h1 {
              font-size: 2rem;
              text-transform: uppercase;
              font-family: "Bahnschrift";
              font-weight: 400;
            }

            p {
              line-height: 1.5;
              color: #232323;
              text-align: justify;
            }

            ul {
              list-style: none;
            }


            header{
              color: white;
              background:
                url(https://howtoweb3.de/images/medium/003.jpg);
              background-size: cover;
              background-position: center center;
              border-bottom: 0.5rem solid #1e64e2;
              padding: 0 1rem 1rem;
              margin-bottom: 3rem;
              height: 20rem;
              box-shadow: 0 0 1rem rgba(0,0,0,0.8);
            }

            nav ul{
              font-size: 1rem;
              list-style: none;
              margin: 0;
              padding: 0;
              text-align: right;
            }

            .centered {
              text-align: center;
            }

            main{
              margin: 2rem auto;
              padding: 0 2rem;
              width: 100%;
              max-width: 40rem;
            }

            th, td {
              border-bottom: 0.125rem solid #232323;
            }

            img {
              width:100%;
              margin: 1rem 0; 
              max-height: 15rem;
              object-fit: cover;
              box-shadow: 0 0 1rem rgba(0,0,0,0.6);
              border-radius: 1rem;
            }

            nav li {
              display:inline-block;
            }
            nav li a{
              display: block;
              padding: 1rem;
              color: #fff;
              text-decoration: none;
            }

          header
            nav
              ul
                li
                  a(tabindex="0" aria-current="page") Startseite
                li
                  a(href="html/preise.html") Preise
                li
                  a(href="html/bilder.html") Bilder
            h1.logo Doofenshmirtz Evil Incorporated
          main
            h3 Jingle
            audio(src="./media/doofenschmirtz.mp3", controls, style="width:100%")
